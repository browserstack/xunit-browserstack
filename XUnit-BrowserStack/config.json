{
  "server": "hub.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "build": "browserstack build",
    "name": "BStack sample test",
    "browserstack.debug": true,
    "browserstack.source": "xunit:sample-selenium-3:v1.0"
  },

  "environments": [
    {
      "browser": "chrome"
    },
    {
      "browser": "firefox"
    },
    {
      "browser": "safari"
    }
  ],

  "localOptions": {
    "forceLocal": true
  }
}
